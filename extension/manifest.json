{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": ["lib/jquery.js", "lib/exif-parser-0.1.9.js", "content_script/page_analyzer.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "description": "GeoDeck",
   "manifest_version": 2,
   "name": "Semantic Geo",
   "permissions": [ "http://*/*", "https://*/*", "management", "storage", "cookies", "tabs", "webRequest", "webRequestBlocking" ],
   "version": "1.0.0.1"
}
